{
  "name": "vuecomponentt1",
  "version": "1.0.1",
  "description": "一个基于Vue3的组件库",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && npm run build:es && npm run build:cjs && npm run build:umd",
    "build:es": "vite build --config vite.config.es.ts",
    "build:cjs": "vite build --config vite.config.cjs.ts",
    "build:umd": "vite build --config vite.config.umd.ts",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "typescript"
  ],
  "dependencies": {
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "typescript": "^5.6.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.4.0",
    "vue-tsc": "^2.1.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  }
}